programa {
  funcao inicio() {
    cadeia cor

    escreva("qual a cor do produto?")
    leia(cor)

    se(cor == "verde")
    escreva("preco = R$10")

    se(cor == "azul")
    escreva("preco = R$20")

    se(cor == "amarelo")
    escreva("preco = R$30")

    se(cor == "vermelho")
    escreva("preco = R$40")

  }
}
