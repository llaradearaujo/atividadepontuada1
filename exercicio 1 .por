programa {
  funcao inicio() {
    inteiro A, B, C 

    escreva("escreva o valor de A:")
    leia(A)

    escreva("escreva o valor de B:")
    leia(B)

    escreva("escreva o valor de C:")
    leia(C)

    se(A+B < C) 
    escreva("soma de A e B  menor que C!")
    senao
    escreva ("soma de A e B maior que C!")

  }
}
